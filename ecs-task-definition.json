{
  "family": "ec2demo-task",
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "ec2demo",
      "image": "720628977237.dkr.ecr.us-east-2.amazonaws.com/ec2demo:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "EC2"
  ],
  "memory": "512",
  "cpu": "256"
}
